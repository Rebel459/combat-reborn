{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "hidden",
        "model": {
          "type": "minecraft:empty"
        }
      },
      {
        "when": "empty",
        "model": {
          "type": "minecraft:model",
          "model": "combat_reborn:item/quiver_empty"
        }
      },
      {
        "when": "one",
        "model": {
          "type": "minecraft:model",
          "model": "combat_reborn:item/quiver_1"
        }
      },
      {
        "when": "two",
        "model": {
          "type": "minecraft:model",
          "model": "combat_reborn:item/quiver_2"
        }
      },
      {
        "when": "three",
        "model": {
          "type": "minecraft:model",
          "model": "combat_reborn:item/quiver_3"
        }
      },
      {
        "when": "full",
        "model": {
          "type": "minecraft:model",
          "model": "combat_reborn:item/quiver_full"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "combat_reborn:item/quiver_full"
    }
  }
}